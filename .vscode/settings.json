{
  "cSpell.words": [
    "Ihand",
    "Imove",
    "Ipart"
  ],
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "$(capture).js, $(capture).*.ts",
    "*.js": "$(capture).js.map, $(capture).min.js, $(capture).d.ts",
    "*.jsx": "$(capture).js",
    "*.tsx": "$(capture).ts",
    "package.json": "*.config.js, tsconfig.json, .*rc.js, package-lock.json, .npmrc,  makefile, .editorconfig, .*ignore,"
  }
}
